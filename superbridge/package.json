{
  "name": "superbridge",
  "version": "0.0.1",
  "type": "module",
  "exports": {
    "./preload": {
      "types": "./dist/preload/index.d.ts",
      "import": "./dist/preload/index.js",
      "require": "./dist/preload/index.cjs"
    },
    "./main": {
      "types": "./dist/main/index.d.ts",
      "import": "./dist/main/index.js",
      "require": "./dist/main/index.cjs"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.cjs"
    },
    ".": {
      "types": "./dist/shared/index.d.ts",
      "import": "./dist/shared/index.js",
      "require": "./dist/shared/index.cjs"
    }
  },
  "main": "./dist/shared/index.cjs",
  "module": "./dist/shared/index.js",
  "types": "./dist/shared/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node  --loader ts-node/esm ./vite/build.ts",
    "dev": "node  --loader ts-node/esm ./vite/dev.ts",
    "typecheck": "tsc --noEmit",
    "ts:types": "tsc --watch --project tsconfig.json"
  },
  "dependencies": {
    "superjson": "^2.2.2"
  },
  "peerDependencies": {
    "electron": "*"
  },
  "devDependencies": {
    "execa": "^8.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0"
  }
}
